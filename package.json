{
  "name": "paper-tree-monorepo",
  "version": "0.2.0",
  "description": "Fast, flexible Node.js static site generator.",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "start": "node -r dotenv/config .",
    "dev": "NODE_ENV=development node -r dotenv/config .",
    "type-check": "npm run type-check --workspaces --if-present",
    "test": "vitest run --coverage",
    "build": "npm run build --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present && rimraf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gryphonmyers/paper-tree.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "html",
    "static",
    "site",
    "generator"
  ],
  "author": "Gryphon Myers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gryphonmyers/paper-tree/issues"
  },
  "homepage": "https://github.com/gryphonmyers/paper-tree#readme",
  "dependencies": {
  },
  "devDependencies": {
    "husky": "^8.0.3"
  }
}
